config_version=5

[application]

config/name="MC Wizards"
config/version="0.1.0"
run/main_scene="res://scenes/arena_main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/textures/logo/MC_logo_full_inverted.png"
config/icon="res://assets/textures/logo/MC_logo_res_half.png"

[autoload]

DiscordSDKLoader="*res://addons/discord-sdk-gd/nodes/core_updater.gd"
NetworkUtils="*res://scripts/globals/NetworkUtils.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/borderless=true

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-sdk-gd/plugin.cfg", "res://addons/mouton_fsm/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "teal",
"res://scenes/": "red",
"res://scripts/": "blue"
}

[filesystem]

import/fbx/enabled=false

